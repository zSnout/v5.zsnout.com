    "@types/ace": "^0.0.47",
    "@types/bcrypt": "^5.0.0",
    "@types/chess.js": "^0.11.2",
    "@types/chessboardjs": "^0.3.2",
    "@types/ejs": "^3.1.0",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^16.11.11",
    "@types/nodemailer": "^6.4.4",
    "@types/uglify-js": "^3.13.1",
    "bcrypt": "^5.0.1",
    "ejs": "^3.1.6",
    "fast-glob": "^3.2.7",
    "fastify": "^3.24.0",
    "fastify-static": "^4.5.0",
    "markdown-it": "^12.2.0",
    "markdown-it-anchor": "^8.4.1",
    "markdown-it-meta": "^0.0.1",
    "mathjax": "^3.2.0",
    "mongodb": "^4.1.4",
    "nodemailer": "^6.7.1",
    "prettier": "^2.5.0",
    "sass": "^1.43.4",
    "socket.io": "^4.3.2",
    "typescript": "4.5.2",
    "uglify-js": "^3.14.4"
  }
}
